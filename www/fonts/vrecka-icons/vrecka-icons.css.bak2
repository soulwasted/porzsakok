		url('iconfont.woff2?1507267465489') format('woff2'),
		url('iconfont.woff?1507267465489') format('woff'),
		url('iconfont.ttf?1507267465489') format('truetype');
	font-weight: normal;
	font-style: normal;
}

[class^='icon-']:before,
[class*=' icon-']:before {
	font-family: 'iconfont';
	display: inline-block;
	vertical-align: middle;
	line-height: 1;
	font-weight: normal;
